{% extends 'base.html.twig' %}

{% block title %}Organisatie-gegevens aanpassen{% endblock %}

{% block body %}
	<header class="center">
    	<h1>Gegevens {{ organisation.name }} aanpassen</h1>
    </header>

    <section id="maakVereniging" class="steps">
	    {% embed "organisation/vereniging_form.html.twig" with { 'form': form, 'organisation': organisation, "source": "create_organisation" } %}{% endembed %}
	</section>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset("js/forms.js") }}"></script>
{% endblock %}

