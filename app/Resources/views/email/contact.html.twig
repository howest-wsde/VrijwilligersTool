{% trans_default_domain "mail" %}
{% extends "emailbase.html.twig" %}

{% block body %}
	<dl>
		<dt>{{ "contact.name"|trans }} </dt>
		<dd>{{ contact.name }}</dd>
		<dt>E{{ "contact.email"|trans }} </dt>
		<dd>{{ contact.email }}</dd>
		<dt>{{ "contact.telephone" | trans }} </dt>
		<dd>{{ contact.telephone }}</dd>
		<dt>{{ "contact.message" | trans }} </dt>
		<dd>{{ contact.description }}</dd>
	</dl>
{% endblock %}