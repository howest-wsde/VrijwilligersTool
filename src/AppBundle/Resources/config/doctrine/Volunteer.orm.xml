<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Volunteer" table="volunteer">
    <indexes>
      <index name="FKUser301874" columns="Contact"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="Id" columns="Id"/>
    </unique-constraints>
    <id name="id" type="integer" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="FirstName" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="LastName" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="username" type="string" column="username" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passphrase" type="string" column="passphrase" length="60" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastUpdate" type="datetime" column="last_update" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="contact" target-entity="Contact" fetch="LAZY">
        <cascade><cascade-persist/></cascade>
      <join-columns>
        <join-column name="Contact" referenced-column-name="Id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="skillproficiency" target-entity="Skillproficiency" inversed-by="volunteer" fetch="LAZY">
      <join-table name="volunteer_has_skillproficiency">
        <join-columns>
          <join-column name="volunteer_Id" referenced-column-name="Id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="skillproficiency_Id" referenced-column-name="Id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
