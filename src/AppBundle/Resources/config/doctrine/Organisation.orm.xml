<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Organisation" table="organisation">
    <indexes>
      <index name="FKOrganisati891792" columns="Contact"/>
      <index name="FKOrganisati829755" columns="Creator"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="Name" columns="Name"/>
      <unique-constraint name="Id" columns="Id"/>
    </unique-constraints>
    <id name="id" type="integer" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="Name" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="Description" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="creator" target-entity="Volunteer" fetch="LAZY">
      <join-columns>
        <join-column name="Creator" referenced-column-name="Id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="contact" target-entity="Contact" fetch="LAZY">
      <join-columns>
        <join-column name="Contact" referenced-column-name="Id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
